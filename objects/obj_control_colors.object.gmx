//Transition theme colors gradually
if ColorEase[0] != 0{

    // if color changed mid transition, use in flux colors instead of previous skin index
    if ColorPrevNew{
        //Grab Prev Colors
        for(var i=0; i &lt; SKIN_NAME_INDEX+1; i++){
            COLORSPREV[i] = COLORS[i]
        }
        ColorPrevNew = false;
    }
    
    
    //Ease Colors towards new skin
    for(var i=0; i &lt; SKIN_NAME_INDEX; i++){
        if !SKIN_LOCKED_FLAG or i &gt;= 6{
            COLORS[i] = merge_colour(COLORSPREV[i], mColors[i,CURSKIN],ColorEase[0]);
            //I use colors prev so that the color ease doesn't accelerate.
            //i.e. if you're merging with a merge color it accelerates.
        }
        else{
            COLORS[i] = merge_colour(COLORSPREV[i], mColors[0,CURSKIN],ColorEase[0]);
        }
    }
    
    //Set Skin Name
    COLORS[SKIN_NAME_INDEX] = mColors[SKIN_NAME_INDEX,CURSKIN];
    
    //Set Background Color
    window_set_colour(COLORS[6]);

    //THEME COLOR CHANGER caller
    if mColorsChanger != 0 and !instance_exists(obj_control_pause){
        scr_change_theme()
    }
}
//Reset color tween vars
