if keyboard_check(vk_shift){

    
    var is_Pressed = 0;
    if mouse_check_button_released(mb_right) {
        is_Pressed = 1;
    }
    else if  mouse_check_button_released(mb_left){
        is_Pressed = SKIN_COUNT - 1;
    }
    //Quick theme changer
    if is_Pressed != 0
    {
    
        //Change current theme
        CURSKIN = (CURSKIN + is_Pressed) mod SKIN_COUNT;
        
        //Save Skin Change
        mColorsChanger = true;
        if GAME_PAUSE {mColorsChanger = true;}
        SWIPE_TAP = false;
        
        ini_open("scores.ini")
            ini_write_real("settings", "CURSKIN", CURSKIN);
        ini_close()

        
        
        //Color Easer
        scr_color_easer(.5)
        
        
    }
    
    /// Toggle Color Series
    if keyboard_check_pressed(vk_space){
        COLOR_SERIES = !COLOR_SERIES;
    }
}

