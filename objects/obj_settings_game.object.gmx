    <event eventtype="7" enumb="10">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Game Settings Choices


switch selected[0]
{

    //Arrow GridSize
    case 1: //see obj dropdown box
    
        
        CUR_GRID = scr_modular_delta(CUR_GRID, ADJUST, UNLOCKS_DATA_SIZES[0]);
        
        
        //If Unlocked
        var data = scr_unlock_get_data(0,CUR_GRID);
        if data[1] != 0{
            ini_open("scores.ini")
                ini_write_real("settings", "GRID", CUR_GRID);
                //Update New Counts
                scr_settings_update_counts(data,COUNTS_GRID)
                //Increment Saved Views
                scr_unlock_update_views_file(data);
            ini_close()
        }
        
        // Track Selections
        analytics_button_counter("Options-Grid","GRID,"+string(CUR_GRID));
        
            
        selected[@ 0] = noone;
        break;
        
    //Arrow Rigor
    case 2: //see obj dropdown box
        
        CUR_RIGOR = scr_modular_delta(CUR_RIGOR, ADJUST, UNLOCKS_DATA_SIZES[2]);
        
        //If Unlocked   
        var data = scr_unlock_get_data(2,CUR_RIGOR);
        if data[1] != 0{
            ini_open("scores.ini")
                ini_write_real("settings", "RIGOR", CUR_RIGOR);
                //Update New Counts
                scr_settings_update_counts(data,COUNTS_RIGOR)
                //Increment Saved Views
                scr_unlock_update_views_file(data);
            ini_close()
        }
        
        // Track Selections
        analytics_button_counter("Options-Rigor","RIGOR,"+string(CUR_RIGOR));
        
            
        selected[@ 0] = noone;
        break;
   
      
    //Arrow Touch Mode
    case 3:
    
        TP_SENSE[@ 1] = scr_modular_delta(TP_SENSE[1], ADJUST, array_length_1d(tm_names));
        
        ini_open("scores.ini")
            ini_write_real("settings", "TP_MODE", TP_SENSE[1]);
        ini_close()
     
        // Track Selections
        analytics_button_counter("Options-Controls","TOUCH_MODE,"+string(TP_SENSE[1]));
        
        selected[0] = noone;
        break;

    //Toggle Joystick Display
    case 4:
    
        TOUCH_PAD_SHOW = !TOUCH_PAD_SHOW;
        
        ini_open("scores.ini")
            ini_write_real("settings", "TOUCH_PAD_SHOW", TOUCH_PAD_SHOW);
        ini_close()
        // Track Selections
        analytics_button_counter("Options-ControlsVisible","ControlsVisible,"+string(TOUCH_PAD_SHOW));
    
        selected[0] = noone;
        break;
   
}
</string>
