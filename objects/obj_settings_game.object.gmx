            <kind>1</kind>
            <string>///Game Settings Page


title_txt = "game";


tp_show_spr[0] = spr_TP_hide;
tp_show_spr[1] = spr_TP_show;
/*
//Toggle Sprites
touchMode_spr[0] = s_v_TM_Fluid;
touchMode_spr[1] = s_v_TM_Fixed;
touchMode_spr[2] = s_v_TM_Line;
for (var i = 0, n = array_length_1d(RMSPD_OPTIONS); i &lt; n; i++){
    sense_spr[i] = asset_get_index("s_v_FPS_"+string(RMSPD_OPTIONS[i]));
}*/
// TouchMode Names
var i = -1;
tm_names[++i] = "fluid";
tm_names[++i] = "fixed";
tm_names[++i] = "linear";


// Menu Selection Vars
selected[0] = noone;
selected[1] = true;







var i;
i = -1; 
// Set Game Settings Items
menu[++i] = scr_create_array("grid size", 1, 0, 1); //name, type, subtype, userevent
menu[++i] = scr_create_array("rigor",     1, 2, 2); //name, type, subtype, userevent
menu[++i] = scr_create_array("controls",     1, -2, 3); //name, type, subtype, userevent
if touchPad != 0{
    menu[++i] = scr_create_array("sensitivity", 2, 2, 4); //name, type, subtype, userevent
}




//Open Stats File
ini_open("scores.ini");
    // Get Counts Data for Unlocks
    COUNTS_GRID = scr_unlock_get_type_counts("0");
    //Set Current Selection
    CUR_GRID = ini_read_real("settings", "GRID", 0);
    
    
    // Get Counts Data for Unlocks
    COUNTS_RIGOR = scr_unlock_get_type_counts("2");
    //Set Current Selection
    CUR_RIGOR = ini_read_real("settings", "RIGOR", 0);
    
    // Set Unlocks Criteria Text
    scr_unlock_set_description("0,2");
//Close Stats File
ini_close();

// Adjustor for Indexes
ADJUST = 0;
//Set Current Grid/Rigor as "Seen" not "New"
scr_delayed_selection(selected,1);
scr_delayed_selection(selected,2);




/// Fonts
NameFont = fnt_menu_bn_26_italic 
DescriptionFont = fnt_menu_bn_15_italic
CountsFont = fnt_menu_bn_26_black






</string>
